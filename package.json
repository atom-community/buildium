{
  "name": "buildium",
  "version": "0.71.1",
  "description": "Build your current project, directly from Atom",
  "keywords": [
    "build",
    "compile",
    "gulp",
    "make",
    "productivity"
  ],
  "repository": "https://github.com/idleberg/atom-buildium",
  "main": "./lib/build",
  "scripts": {
    "build": "npm run clean && rollup --config",
    "dev": "npm run start",
    "clean": "rimraf ./lib",
    "lint:js": "eslint ./src --ignore-path .gitignore",
    "lint:md": "remark . --quiet --frail --ignore-path .gitignore",
    "lint": "npm-run-all --parallel lint:*",
    "start": "rollup --watch --config",
    "test": "eslint ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --allow-empty"
    }
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,json,md}": "prettier --write"
  },
  "dependencies": {
    "atom-package-deps": "^4.0.1",
    "atom-space-pen-views": "^2.0.3",
    "cross-spawn": "^4.0.2",
    "cson-parser": "^1.3.0",
    "js-yaml": "^3.4.6",
    "term.js": "https://github.com/jeremyramin/term.js/tarball/de1635fc2695e7d8165012d3b1d007d7ce60eea2",
    "tree-kill": "^1.0.0",
    "xregexp": "^3.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/plugin-proposal-function-bind": "^7.12.13",
    "@babel/plugin-syntax-function-bind": "^7.12.13",
    "@babel/preset-env": "^7.13.10",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@stylelint/prettier-config": "^2.0.0",
    "@stylelint/remark-preset": "^2.0.0",
    "atom-build-spec-helpers": "^0.4.0",
    "eslint": "^2.10.1",
    "eslint-config-atom-build": "^3.0.0",
    "fs-extra": "^2.1.2",
    "husky": "^4.3.8",
    "jsonlint": "^1.6.3",
    "lint-staged": "^10.5.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "remark-cli": "^9.0.0",
    "rimraf": "^2.6.3",
    "rollup": "^2.41.5",
    "rollup-plugin-terser": "^7.0.2",
    "stylelint": "^13.12.0",
    "temp": "^0.8.1"
  },
  "engines": {
    "atom": ">=1.0.0"
  },
  "activationHooks": [
    "core:loaded-shell-environment"
  ],
  "consumedServices": {
    "builder": {
      "versions": {
        "^2.0.0": "consumeBuilder"
      }
    },
    "status-bar": {
      "versions": {
        "^1.0.0": "consumeStatusBar"
      }
    },
    "busy-signal": {
      "versions": {
        "^1.0.0": "consumeBusySignal"
      }
    },
    "linter-indie": {
      "versions": {
        "1.0.0": "consumeLinterRegistry"
      }
    }
  },
  "package-deps": [
    "busy-signal"
  ]
}
